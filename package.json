    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "update": "ncu -ws -i"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^9.24.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "turbo": "^2.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  },
  "dependencies": {
    "@actions/core": "^1.11.1"
  }
}
