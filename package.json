{
    "name": "action-release-download",
    "version": "1.0.0",
    "author": "romnn <contact@romnn.com>",
    "license": "MIT",
    "description": "Download and cache github action release binaries",
    "scripts": {
        "build": "tsc",
        "check": "tsc --noEmit",
        "format": "prettier --write",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
        "test": "jest",
        "test:cov": "jest --collectCoverage"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "/dist"
    ],
    "dependencies": {
        "@actions/cache": "^3.0.6",
        "@actions/tool-cache": "^2.0.1",
        "@octokit/core": "^4.1.0",
        "@types/node": "^18.11.9",
        "toml": "^3.0.0"
    },
    "devDependencies": {
        "@types/jest": "^29.2.3",
        "@types/tmp": "^0.2.3",
        "jest": "^29.3.1",
        "tmp": "^0.2.1",
        "ts-jest": "^29.0.3",
        "typescript": "^4.9.3"
    }
}
