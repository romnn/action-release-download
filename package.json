{
  "name": "action-get-release-workspace",
  "license": "MIT",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "./action-get-release",
      "./action"
    ]
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "npm@10.8.1",
  "scripts": {
    "check": "turbo run check",
